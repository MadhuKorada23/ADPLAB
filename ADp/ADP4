#4A

import numpy as np
a=np.arange(10,1,-2)
print(a)
b=a[np.array([3,1,2])]
print(b)
c=np.arange(0,10,1)
print(c)
d=c[5:8:1]
print(d)
c[1:]
c[c>4]
c[c%2==0]**2


#4B


import numpy as np
import statistics as st
a=np.array([[5,9,1],[4,8,1],[1,6,8]])
print(a)
print(np.min(a))
print(np.min(a,1))
print(np.max(a))
print(np.max(a,1))
print(np.mean(a))
print(np.median(a))
print(np.std(a))
b=np.array([5,6,2,3,9,8,7,1])
print(st.mode(b))
